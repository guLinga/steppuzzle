{
  "name": "steppuzzle",
  "version": "0.0.2",
  "description": "A Generation Static Blog Framework 一个静态博客框架",
  "main": "dist/tsup/index.js",
  "module": "dist/tsup/index.mjs",
  "types": "dist/tsup/index.d.ts",
  "scripts": {
    "build": "steppuzzle build docs",
    "dev": "steppuzzle dev docs",
    "ts:build": "echo 'start typesctipt build' && tsc",
    "ts:run": "echo 'start typescript run' && tsc --watch",
    "cli:test": "echo 'cli run' && pnpm ts:start && node dist/node/cli",
    "ssg:run": "echo 'ssg build' && pnpm serve docs/build",
    "tsup:build": "echo 'start tsup build' && tsup",
    "tsup:run": "echo 'start tsup run' && tsup --watch",
    "server:run": "echo 'localhost serve' && pnpm build && pnpm serve docs/build",
    "eslint:run": "echo 'run eslint and prettier' && eslint --fix --ext .ts,.tsx,.js,.jsx --quiet ./",
    "test:init": "vitest run",
    "test:ui": "vitest --ui",
    "prepare:e2e": "tsx scripts/prepare-e2e.ts",
    "test:e2e": "playwright test",
    "release": "tsx ./scripts/release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test:release": "pnpm release --dry"
  },
  "bin": {
    "steppuzzle": "bin/steppuzzle.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "eslint --fix"
    ]
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "README.zh-CN.md",
    "src/runtime",
    "src/theme-default",
    "src/shared"
  ],
  "keywords": [
    "vite",
    "ssg"
  ],
  "author": "GuLinga",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/helper-plugin-utils": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/traverse": "^7.21.5",
    "@loadable/component": "^5.15.2",
    "@mdx-js/rollup": "2.1.3",
    "@vitejs/plugin-react": "^2.2.0",
    "acorn": "^8.8.2",
    "cac": "^6.7.14",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "github-slugger": "^2.0.0",
    "hast-util-from-html": "^1.0.2",
    "lodash-es": "^4.17.21",
    "mdast-util-mdxjs-esm": "^1.3.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx-frontmatter": "^3.0.0",
    "rollup": "^3.21.7",
    "shiki": "^0.14.2",
    "sirv": "^2.0.3",
    "unocss": "^0.48.0",
    "vite": "^4.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/carbon": "^1.1.16",
    "@playwright/test": "1.26.1",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__traverse": "^7.18.5",
    "@types/fs-extra": "^11.0.1",
    "@types/hast": "^2.3.4",
    "@types/lodash-es": "^4.17.7",
    "@types/mdast": "^3.0.11",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/ui": "^0.31.0",
    "chalk": "^5.2.0",
    "commitlint": "^17.6.3",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
